{
    "name": "felang",
    "displayName": "Fe lang",
    "description": "Fe lang for Visual Studio Code",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.68.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "publisher": "vuvoth",
    "repository": {
        "type": "git",
        "url": "https://github.com/vuvoth/vscode-felang"
    },
    "contributes": {
        "languages": [
            {
                "id": "fe-lang",
                "aliases": [
                    "Fe lang ",
                    "fe-lang"
                ],
                "extensions": [
                    ".fe"
                ],
                "configuration": "./language-configuration.json",
                "icon": {
                    "light": "./icons/fe_source.svg",
                    "dark": "./icons/fe_source.svg"
                }
            }
        ],
        "grammars": [
            {
                "language": "fe-lang",
                "scopeName": "source.fe",
                "path": "./syntaxes/fe-lang.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "fe-lang",
                "path": "./snippets/felang.json"
            }
        ]
    }
}
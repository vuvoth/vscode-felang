{
  "name": "felang",
  "displayName": "felang",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:fe-lang"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "felang.helloWorld",
        "title": "Hello World"
      }
    ],
    "languages": [
      {
        "id": "fe-lang",
        "aliases": [
          "Fe lang ",
          "fe-lang"
        ],
        "extensions": [
          ".fe"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icons/fe_source.svg",
          "dark": "./icons/fe_source.svg"
        }
      }
    ],
    "grammars": [
      {
        "language": "fe-lang",
        "scopeName": "source.fe",
        "path": "./syntaxes/fe-lang.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "fe-lang",
        "path": "./snippets/felang.json"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "fe-language-server",
      "properties": {
        "fe-language-server.trace.server": {
          "type": "string",
          "scope": "window",
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "enumDescriptions": [
            "No traces",
            "Error only",
            "Full log"
          ],
          "default": "on",
          "description": "Traces the communication between VS Code and the language server."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.68.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.16.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.2",
    "@vscode/test-electron": "^2.1.3"
  }
}